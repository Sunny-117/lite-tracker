  "main": "index.js",
  "scripts": {
    "warning": "echo \"\\033[31m 由于babel-node绑定async函数的this时有bug, 不能直接使用babel-node  \\033[0m \" && echo \"\\033[31m 因此, 请确保本方法前已执行了'npm run watch'  \\033[0m \" && echo \"\" ",
    "watch": "babel src --copy-files --watch --out-dir dist  --ignore src/public --verbose --source-maps",
    "test": "mocha --timeout 5000 --reporter mochawesome dist/test/*.js",
    "test:cover": "istanbul cover dist/test/*.js",
    "reporter": "mocha --reporter mochawesome dist/test/*.js",
    "build": "babel src --copy-files -d dist --verbose",
    "production": "NODE_ENV=production node dist/app.js",
    "start": "npm run dev",
    "dev": "SET NODE_ENV=development nodemon dist/app.js",
    "fee": "NODE_ENV=development node dist/fee.js",
    "test_fee": "NODE_ENV=testing node dist/fee.js",
    "test_start": "NODE_ENV=testing node dist/app.js",
    "prod_fee": "NODE_ENV=production node dist/fee.js",
    "prod_start": "NODE_ENV=production node dist/app.js"
  },
